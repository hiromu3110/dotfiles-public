Host bitbucket
  HostName bitbucket.org
  User git

Host github
  HostName github.com
  User git

Host ssh.ng.bluemix.net
  DynamicForward 1080
  ControlMaster no

Host *
  #IdentityFile ~/.ssh/id_rsa
  #IdentityFile ~/.ssh/id_ecdsa
  ServerAliveInterval 300
  ControlMaster auto
  ControlPath ~/.ssh/mux-%C.sock
  ControlPersist 1
